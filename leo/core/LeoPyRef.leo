    """Repeat the last command."""
    if self.in_dot:
        return
    try:
        self.in_dot = True
        # Save the dot list.
        self.old_dot_list = self.dot_list[:]
        # Copy the list so it can't change in the loop.
        for event in self.dot_list[:]:
